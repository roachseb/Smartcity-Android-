        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.Accueil"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Inscription" />
        <activity android:name=".Activity.ProfilActivity" />
        <activity android:name=".Activity.AjoutReseau" />
        <activity android:name=".Activity.RechercheReseau" />
